msgstr ""

#: babel.units.format_compound_unit:41 babel.units.format_unit:45
#: babel.units.get_unit_name:18 of
msgid "the `Locale` object or locale identifier"
msgstr ""

#: babel.units.format_compound_unit:1 of
msgid "Format a compound number value, i.e. \"kilometers per hour\" or similar."
msgstr ""

#: babel.units.format_compound_unit:3 of
msgid ""
"Both unit specifiers are optional to allow for formatting of arbitrary "
"values still according to the locale's general \"per\" formatting "
"specifier."
msgstr ""

#: babel.units.format_compound_unit:31 of
msgid "See https://www.unicode.org/reports/tr35/tr35-general.html#perUnitPatterns"
msgstr ""

#: babel.units.format_compound_unit:33 of
msgid ""
"The numerator value. This may be a string, in which case it is considered"
" preformatted and the unit is ignored."
msgstr ""

#: babel.units.format_compound_unit:35 of
msgid "The numerator unit. See `format_unit`."
msgstr ""

#: babel.units.format_compound_unit:36 of
msgid ""
"The denominator value. This may be a string, in which case it is "
"considered preformatted and the unit is ignored."
msgstr ""

#: babel.units.format_compound_unit:38 of
msgid "The denominator unit. See `format_unit`."
msgstr ""

#: babel.units.format_compound_unit:39 of
msgid "The formatting length. \"short\", \"long\" or \"narrow\""
msgstr ""

